{
  "name": "transpiler",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "pnpm build && node dist/index.js",
    "build:defs": "tsx tools/generate-def.ts",
    "client-type": "dts-bundle-generator src/client.ts -o dist/client.d.ts --external-inlines runtypes --inline-declare-global",
    "build": "rollup -c",
    "build:all": "rimraf dist && rollup -c && pnpm client-type"
  },
  "dependencies": {
    "@babel/standalone": "^7.20.13",
    "runtypes": "^6.6.0"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__standalone": "^7.1.4",
    "@types/babel__traverse": "^7.18.3",
    "@types/node": "^18.11.18",
    "dts-bundle-generator": "^7.1.0",
    "peggy": "^2.0.1",
    "rimraf": "^4.1.2",
    "rollup": "^3.9.1",
    "tslib": "^2.4.1",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  }
}
